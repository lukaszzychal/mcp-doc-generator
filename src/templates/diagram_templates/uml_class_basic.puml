    - {{entity1_field1}}: {{entity1_field1_type}}
    - {{entity1_field2}}: {{entity1_field2_type}}
    + {{entity1_method1}}()
    + {{entity1_method2}}()
}

class {{entity2_name}} {
    - id: {{id_type}}
    - {{entity2_field1}}: {{entity2_field1_type}}
    - {{entity2_field2}}: {{entity2_field2_type}}
    + {{entity2_method1}}()
    + {{entity2_method2}}()
}

class {{entity3_name}} {
    - id: {{id_type}}
    - {{entity3_field1}}: {{entity3_field1_type}}
    + {{entity3_method1}}()
}

